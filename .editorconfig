# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Matches multiple files with brace expansion notation
# 编码格式
charset = utf-8

# space 缩进
indent_style = space
indent_size = 2

# 去除行尾空白字符
trim_trailing_whitespace = true

# 文件结尾符
# end_of_line = lf
# insert_final_newline = true
